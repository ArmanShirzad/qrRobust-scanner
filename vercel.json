{
  "version": 2,
  "name": "qr-designer-v2",
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app/main.py"
    },
    {
      "src": "/docs",
      "dest": "/app/main.py"
    },
    {
      "src": "/redoc",
      "dest": "/app/main.py"
    },
    {
      "src": "/health",
      "dest": "/app/main.py"
    },
    {
      "src": "/openapi.json",
      "dest": "/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "functions": {
    "app/main.py": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "18"
    }
  }
}